{
  "manifest_version": 3,

  "name": "Tab Dragon Extension",
  "description": "This extension will save collections of tabs for later consumption",
  "version": "1.0",

  "action": {
    "default_icon": "./src/img/dragon/dragon (10).png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "host_permissions": []
}
